{
  "expo": {
    "name": "speedreader",
    "slug": "spdrdr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Miami_potato_square.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Miami_potato.png",
      "resizeMode": "contain",
      "backgroundColor": "#eee"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "usesIcloudStorage": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Miami_potato_square.png",
        "backgroundColor": "#eee"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
